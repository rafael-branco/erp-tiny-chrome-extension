{
    "manifest_version": 3,
    "name": "ERP Tiny Automation",
    "description": "Runs a script on erp.tiny.com.br automatically when user installs the extension",
    "version": "1.0.2",
    "content_scripts": [
      {
        "js": [
          "script.js"
        ],
        "matches": [
          "https://erp.tiny.com.br/*",
          "https://accounts.tiny.com.br/*"
        ]
      }
    ]
  }